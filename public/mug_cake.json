{
    "title": "Mug Cake",
    "category": "Dessert",
    "yield": "1",
    "difficulty": "easy",
    "ingredients": ["2 tbsp coconut flour (14g)",
                    "1/2 tbsp collagen (6g)",
                    "1 tbsp flax meal (6.5g)",
                    "1/4 tsp baking powder",
                    "1 tbsp cocoa powder (5g)",
                    "1 egg",
                    "1/3 cup almond milk",
                    "optional: 1 tbsp applesauce"],
    "directions": ["Mix dry and wet ingredients separately",
                    "Combine and mix well",
                    "Microwave for 3 1/2 minutes",
                    "Enjoy!"]
}
